  <a href="/signout" class="navbar-right">Sign Out</a>
  <a href="/profile" class="navbar-right">Profile</a>
  <a href="/post" class="navbar-right">Make New Post</a>
</nav>


<div class="userPosts">

  <% @posts.each do |post| %>
    <h2> <%= post.title %> </h2>
    </br><%="by #{post.user.username}"%>
    <p class="content"> <%= post.content %> </p>
    <p><img class="imgPost" src="<%= post.image %>" ></p>
    <p> <%= "#{post.created_at.strftime("%b %d, %Y")}" %> </p>
  <% end %>

</div>

